}
.target-linksWrapper ul{
	margin:0px;
}
.target-linksWrapper ul li{
	display:inline-block;
	margin:0px 10px;
	border-bottom:2px solid #edf1f1;
	transition:all 0.3s ease;
	-webkit-transition:all 0.3s ease;
	-moz-transition:all 0.3s ease;
}
.target-linksWrapper ul li:hover{
	background:#fff;
	border-bottom:2px solid #56d3f6;
	box-shadow:2px 2px 6px rgba(0,0,0,0.01); 
}
.target-linksWrapper ul li a{
	font-size:13px;
	color:#696969;
	display:block;
	padding:26px 25px;
	font-weight:600;
}
.target-linksWrapper ul li:hover a{
	color:#023f78;
}
.target-linksWrapper ul li:hover .icon-box{
	background-color:#1e93e4;
}
.target-linksWrapper ul li a .icon-box{
	width:50px;
	height:50px;
	display:block;
	margin:0px auto 18px auto;
	border-radius:50%;
	background-color:#8bbadb;
	box-shadow:3px 0px 0px #d8dbdb; 
	background-position:center;
	background-repeat:no-repeat;
	transition:all 0.6s ease;
	-webkit-transition:all 0.6s ease;
	-moz-transition:all 0.6s ease;
}
