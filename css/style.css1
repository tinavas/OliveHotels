	overflow:hidden;
}
.hotel-Slider .rslides_nav {
  position: absolute;
  -webkit-tap-highlight-color: rgba(0,0,0,0);
  top: 52%;
  opacity: 0.5;
  z-index:99;
  padding:15px 26px;
  overflow: hidden;
  text-indent:-200px;
  text-decoration: none;
  height: 61px;
  width: 38px;
  background: transparent url("themes/themes.gif") no-repeat left top;
  margin-top: -45px;
 
  }
